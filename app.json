{
  "name": "tigerpath",
  "scripts": {},
  "env": {
    "ALLOWED_HOSTS": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "TIGERBOOK_API_KEY": {
      "required": true
    },
    "TIGERBOOK_USERNAME": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
